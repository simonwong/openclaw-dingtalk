{
  "name": "dingtalk",
  "version": "0.2.0",
  "type": "module",
  "description": "OpenClaw DingTalk channel plugin",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json",
    "openclaw.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adongguo/openclaw-dingtalk.git"
  },
  "keywords": [
    "openclaw",
    "dingtalk",
    "钉钉",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "dingtalk",
      "label": "DingTalk",
      "selectionLabel": "DingTalk (钉钉)",
      "docsPath": "/channels/dingtalk",
      "docsLabel": "dingtalk",
      "blurb": "钉钉/DingTalk enterprise messaging.",
      "aliases": [
        "dingding"
      ],
      "order": 70
    },
    "install": {
      "npmSpec": "@adongguo/dingtalk",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "openclaw": "^2026.2.2-3",
    "ws": "^8.18.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
